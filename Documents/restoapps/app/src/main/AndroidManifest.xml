<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.project.pudin.myapplication">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />

    <application

        android:allowBackup="true"
        android:icon="@drawable/user"
        android:roundIcon="@drawable/user_night"
        android:supportsRtl="true">
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait"
            android:theme="@style/NoBar">
            <intent-filter>
            <action android:name="android.intent.action.MAIN" />
            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter></activity>
        <activity
            android:name=".Select"
            android:label="Masuk"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".Profile"
            android:label="Profil"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".profile_editor"
            android:label="@string/title_activity_profile_editor"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".MainMenu"
            android:label="Menu Makanan"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme">

        </activity>
        <activity android:name=".PesananActivity" android:theme="@style/AppTheme" android:label="Pesanan Saya"/>
        <activity
            android:name=".keranjang"
            android:theme="@style/AppTheme" />
    </application>

</manifest>